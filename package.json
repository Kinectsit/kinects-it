{
  "name": "react-slingshot",
  "version": "3.0.1",
  "description": "Starter kit for creating apps with React and Redux",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "NODE_ENV=development npm-run-all --parallel start:api-server open:src lint:watch",
    "start:api-server": "nodemon server/server.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint",
    "build": "babel-node tools/build.js && cp -R ./src/assets ./dist",
    "test": "npm run build && mocha tools/testSetup.js src/**/*.spec.js server/**/*.spec.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "6.3.6",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.3.5",
    "body-parser": "1.15.0",
    "chart.js": "2.1.3",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.1",
    "exec": "0.2.1",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "formsy-material-ui": "0.4.3",
    "formsy-react": "^0.18.0",
    "foundation-sites": "6.2.1",
    "http-proxy": "1.13.2",
    "ink-docstrap": "1.1.4",
    "jquery": "2.2.3",
    "jsdoc": "3.4.0",
    "material-ui": "0.15.0",
    "morgan": "1.7.0",
    "node-crontab": "0.0.8",
    "object-assign": "4.0.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "path": "0.12.7",
    "pg": "4.5.5",
    "pg-promise": "4.0.10",
    "proxy-middleware": "0.15.0",
    "react": "15.0.2",
    "react-chartjs": "0.7.3",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "redis": "2.6.0-2",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "request": "2.72.0",
    "sequelize": "3.22.0",
    "url": "0.11.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "connect-history-api-fallback": "1.2.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.0",
